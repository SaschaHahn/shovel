{
    "manifest_version": 3,
    "name": "Shovel",
    "version": "0.6.1",
    "permissions": ["scripting", "activeTab", "storage"],
    "description": "Lightweight Custom Technology Detection Extension",
    "action": {
        "default_icon": "/img/shovel48.png",
        "default_popup": "popup.html",
        "defaul_title": "Shows you the technology."
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "img/shovel16.png",
        "32": "img/shovel32.png",
        "48": "img/shovel48.png",
        "128": "img/shovel.png"
    },
 
    "options_page": "moreOptions.html"
}
